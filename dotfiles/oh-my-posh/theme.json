{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "green": "#2A9D8F",
    "safron": "#E9C46A",
    "tea_green": "#C7EFCF",
    "ultra_violet": "#4F518C",
    "space_cadet": "#907AD6",
    "electric_indigo": "#5E2BFF",
    "periwinkle": "#E3D7FF",
    "sandy_brown": "#F0A868",
    "light_red": "#FE6D73",
    "mint_green": "#C4E4DF",
    "dutch_white": "#F0E4C3",
    "platinum": "#E4E3E0"
