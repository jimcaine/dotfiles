DEFAULT_USER=whoami

export XDG_CONFIG_HOME=~/.config
export TERM="xterm-256color"
export CLICOLOR=1
export EDITOR='nvim'

source $HOME/.zshrc.powerline
source $HOME/.zshrc.oh-my-zsh

source $HOME/.zshrc.java
source $HOME/.zshrc.conda
source $HOME/.zshrc.go

source $HOME/.zshrc.aliases
source $HOME/.zshrc.secrets
